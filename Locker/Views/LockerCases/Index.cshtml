@model IEnumerable<Locker.Models.LockerCase>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.LockerCaseName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.LockerCaseName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.LockerCaseId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.LockerCaseId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.LockerCaseId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
